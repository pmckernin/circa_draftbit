<!-- Form visit start -->
<%= form_with(model: @visit, local: true) do |f| %>
<!-- Form visit end -->

  <!-- Label and input for user_id start -->
  <div class="form-group">
    <%= f.label :user_id %>

    <!-- Input for user_id start -->
    <%= f.text_field :user_id, class: "form-control" %>
    <!-- Input for user_id end -->
  </div>
  <!-- Label and input for user_id end -->

  <!-- Label and input for location_id start -->
  <!-- Input for location_id start -->
  <%= f.hidden_field :location_id, value: @place.id %>
  <!-- Input for location_id end -->
  <!-- Label and input for location_id end -->

  <%= f.submit "Create visit", class: "btn btn-block btn-outline-secondary" %>
<% end %>
